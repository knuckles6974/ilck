<mapper namespace="kr.co.ilck.mapper.LoginMapper">
	<select id="login_ok" parameterType="kr.co.ilck.vo.MemberVo" resultType="kr.co.ilck.vo.MemberVo">
		select name,userid from member where userid=#{userid} and pwd=#{pwd}
	</select>
</mapper>